Adding nodes at the end of a list
// gridsize 1 3 java
Node myFirst = null ;
Node myLast = null ; // 0 1
if ( myLast==null )
{
  myFirst = new Node('A', null);
  myLast = myFirst; 
} else {
  myLast.next = new Node('A', null);
  myLast = myLast.next;
}
if ( myLast==null )
{
  myFirst = new Node('B', null);
  myLast = myFirst;
} else {
  myLast.next = new Node('B', null);
  myLast = myLast.next;
}